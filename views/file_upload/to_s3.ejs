<% layout('layout') %>

<div class="row">
  <div class="col-sm-12">
    <form method="post" enctype="multipart/form-data" action="/file_upload/to_s3">
      <div class="form-group">
        <input type="text" name="text" value="TEXT">
      </div>

      <div class="form-group">
        <input type="file" name="file">
      </div>

      <button type="submit" class="btn btn-info">Submit</button>
    </form>
  </div>
</div>

<hr>

<div class="row">
  <div class="col-sm-12">
    <% if (data) { %>
      <pre class="bg-info"><code><%= beautify(JSON.stringify(data), { indent_size: 2 }) %></code></pre>
    <% } %>
  </div>
</div>

